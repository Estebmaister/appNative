{
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "node ./node_modules/jest/bin/jest.js --watch --coverage=false --changedSince=master",
    "testDebug": "node ./node_modules/jest/bin/jest.js -o --watch --coverage=false",
    "testFinal": "node ./node_modules/jest/bin/jest.js",
    "updateSnapshots": "node ./node_modules/jest/bin/jest.js -u --coverage=false"
  },
  "dependencies": {
    "expo": "~38.0.8",
    "expo-splash-screen": "~0.4.0",
    "expo-status-bar": "^1.0.2",
    "expo-updates": "~0.2.11",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-native": "~0.63.2",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-reanimated": "~2.10.0",
    "react-native-screens": "~2.9.0",
    "react-native-unimodules": "~0.10.1",
    "react-native-web": "~0.13.4"
  },
  "devDependencies": {
    "@babel/core": "~7.10.5",
    "babel-jest": "~26.1.0",
    "jest": "~26.1.0",
    "jest-expo": "^38.0.2",
    "react-test-renderer": "~16.13.1"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.style.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/web-build/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "private": true
}
